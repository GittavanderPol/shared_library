<h1>All books</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Author</th>
      <th scope="col">Title</th>
    </tr>
  </thead>
  <tbody>
      <% @books.each do |book| %>
        <tr>
          <td><%= link_to book.author, book %></td>
          <td><%= link_to book.title, book %></td>
        </tr>
      <% end %>
  </tbody>
</table>
